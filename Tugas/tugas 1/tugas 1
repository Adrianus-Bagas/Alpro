/*

 */
package luas;
import static java.lang.Math.sqrt;
import java.util.Scanner;
/*DARYONO
 */
public class Luas {
    public static void main(String[] args) {
        Scanner input = new Scanner(System.in);
        
        double alas, tinggi, a, tp, p, l, tb ;
        
        System.out.print("Alas segitiga = "); alas = input.nextInt();
        System.out.print("Tinggi segitiga = "); tinggi = input.nextInt();
        
        System.out.print("Panjang Balok = "); p = input.nextInt();
        System.out.print("Lebar Balok = "); l = input.nextInt();
        System.out.print("Tinggi Balok = "); tb = input.nextInt();
        
        System.out.print("Sisi dari alas prisma segitiga sama sisi = "); a = input.nextInt();
        System.out.print("Tinggi prisma = "); tp = input.nextInt();
        
        double t;
        t = sqrt(3)*a/2;
        
        double luas;
        luas =  alas * tinggi/2;
        
        double vb;
        vb = p*l*tb;
        
        double lpb;
        lpb = 2*p*l+2*p*tb+2*l*tb;
        
        double vp;
        vp = a*t*tp/2;
        
        double lpp;
        lpp = 3*a*tp + 2*a*t/2;
                
   
        
        System.out.println("Luas Segitiga = "+luas);
        System.out.println("Voulme Balok = "+vb);
        System.out.println("Luas Permukaan Balok = "+lpb);
        System.out.println("Volume Prisma Segitiga Sama Sisi = "+vp);
        System.out.println("Luas Permukaan Prisma Segitiga Sama Sisi = "+lpp);
    }
    
}
